      "\n",
      "split_var='|'\n",
      "def splitter(str):\n",
      "\treturn str.split(split_var)\n",
      "train2 = [split_var.join(train[i]) for i in range(len(train))]\n",
      "test2 = [split_var.join(test[i]) for i in range(len(test))]\n",
      "tfidf = TfidfVectorizer(min_df = 1, tokenizer=splitter)"
     ]
